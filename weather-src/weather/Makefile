maingui.o: maingui.cpp \
		maingui.h \
		getdata.h \
		ZDataSessionManager.h \
		code.h

moc_ZDataSessionManager.o: moc_ZDataSessionManager.cpp \
		ZDataSessionManager.h

moc_getdata.o: moc_getdata.cpp \
		getdata.h \
		ZDataSessionManager.h

moc_maingui.o: moc_maingui.cpp \
		maingui.h \
		getdata.h \
		ZDataSessionManager.h

moc_ZDataSessionManager.cpp: ZDataSessionManager.h
	$(MOC) ZDataSessionManager.h -o moc_ZDataSessionManager.cpp

moc_getdata.cpp: getdata.h
	$(MOC) getdata.h -o moc_getdata.cpp

moc_maingui.cpp: maingui.h
	$(MOC) maingui.h -o moc_maingui.cpp

